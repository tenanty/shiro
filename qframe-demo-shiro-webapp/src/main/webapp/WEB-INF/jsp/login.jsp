  <body>

    <div class="container">
<shiro:notAuthenticated>
      <form class="form-signin" role="form" action="<c:url value="/login"/>" method="post">
        <h2 class="form-signin-heading">登录</h2>
         <c:if test="${requestScope['exception'] == 'LoginException'}">
	        <p class="form-control">登录失败！</p>
	    </c:if>
        <input type="text" class="form-control" placeholder="请输入用户名" name="username" required="" autofocus="">
        <input type="password" class="form-control" placeholder="请输入密码" name="password" required="">
        <label class="checkbox">
          <input type="checkbox" value="remember-me" name="rememberMe"> 记住我
        </label>
       
        <button class="btn btn-lg btn-primary btn-block" type="submit">登录</button>
      </form>
      
</shiro:notAuthenticated>
    </div>

<shiro:authenticated>
    <c:redirect url="/space"/>
</shiro:authenticated>

    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
  

</body></html>